{
    "label" : "sh",
    "description" : "Bourne Shell",
    "tags" : "posix,sh",

    "shell" : "%(sh)s",
    "shellargs": [ "-c" ],
    "commands" : {
        "sh_shell": "%(env)s {ENV: -i {ENV}} {CWD: -C {CWD}} %(sh)s",
        "sh_echo": "{MSG:(%(echo)s -n {MSG});}{MSG_ERR:(%(echo)s -n {MSG_ERR} 1>&2)}",
        "sh_set": "set {KEY}{VALUE:={VALUE}}"
    }
}
